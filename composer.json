{
    "name": "itanea/typhoon",
    "description": "A powerful backend for your Laravel application",
    "license": "MIT",
    "authors": [
        {
            "name": "ITANÃ©A",
            "email": "frederic.blanc@itanea.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "itanea\\typhoon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "itanea\\typhoon\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
