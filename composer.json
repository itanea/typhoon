{
    "name": "itanea/typhoon",
    "type": "library",
    "description": "A powerful backend for your Laravel application",
    "keywords": [
        "itanea",
        "typhoon"
    ],
    "homepage": "https://github.com/itanea/typhoon",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Blanc",
            "email": "frederic.blanc@itanea.fr",
            "homepage": "https://www.itanea.fr/apprendre-le-developpement-web",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "almasaeed2010/adminlte": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Itanea\\Typhoon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itanea\\Typhoon\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "npm": "npm install",
        "npm-run-dev": "npm run dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itanea\\Typhoon\\TyphoonServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
